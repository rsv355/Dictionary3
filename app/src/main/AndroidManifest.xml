        <activity android:name="com.example.android.newsvocabdictionary.HistoryMeanPageActivity"
            android:screenOrientation="portrait"> </activity>

        <activity android:name="com.example.android.newsvocabdictionary.CategoryMeanPageActivity2"
            android:screenOrientation="portrait"> </activity>



        <activity android:name="com.example.android.newsvocabdictionary.HistoryActivity"
            android:screenOrientation="portrait"> </activity>

        <activity android:name="com.example.android.newsvocabdictionary.MeanPageActivity"
            android:screenOrientation="portrait"> </activity>

