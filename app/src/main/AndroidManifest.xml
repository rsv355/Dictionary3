        <provider
            android:authorities="com.example.android.newsvocabdictionary.SampleRecentSuggestionsProvider"
            android:name="com.example.android.newsvocabdictionary.SampleRecentSuggestionsProvider"/>



        <activity android:name="base.MyDrawerActivity"
            android:screenOrientation="portrait">




